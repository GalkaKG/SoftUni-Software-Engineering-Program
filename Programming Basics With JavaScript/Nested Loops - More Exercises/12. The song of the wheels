function theSongOfTheWheels (input) {
    let m = Number(input[0]);
    let buff = "";
    let counter = 0;
    let number = "";
    let pasword = "";
    let isFound = false;

    for (let a = 1; a <= 9; a++) {
        for (let b = 1; b <= 9; b++) {
            for (let c = 1; c <= 9; c++) {
                for (let d = 1; d <= 9; d++) {

                    if (m === a * b + c * d) {
                        if (a < b && c > d) {
                            counter++;
                            number = "" + a + b + c + d;
                            buff += "" + a + b + c + d + " ";
                            
                            if (counter === 4) {
                                 pasword = number;
                                 isFound = true;

                            }
                        }
                    }
                }
            }

        }
    }
    console.log(buff);

    if (isFound) {
        console.log(`Password: ${pasword}`);
    } else {
        console.log("No!");
    }
}
